{
   "name": "css3-colors",
   "version": "1.0.0",
   "description": "Colors classess for css3",
   "main": "index.css",
   "scripts": {
      "dev": "node-sass --watch --include-path scss index.scss index.css --output-style expanded --source-map-embed true",
      "build": "yarn build:expanded & yarn build:min",
      "build:expanded": "node-sass --include-path scss index.scss index.css --output-style expanded",
      "build:min": "node-sass --include-path scss index.scss index.min.css --output-style compressed"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/yoicalsin/css3-colors.git"
   },
   "keywords": [
      "colors",
      "sass-colors",
      "css3"
   ],
   "author": "Yoni Calsin <helloyonicb@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/yoicalsin/css3-colors/issues"
   },
   "homepage": "https://github.com/yoicalsin/css3-colors#readme",
   "devDependencies": {
      "node-sass": "^4.13.1",
      "prettier": "^1.19.1",
      "sass-colors": "^1.0.11"
   }
}
